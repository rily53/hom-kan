<div class="home-left">

  <div class="tag-cell">
    <div class="tag-left">
      <div class="tag-title">TASK</div>
      <div class="tag-addition">
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-menu">
        <path d="M3 12h18M3 6h18M3 18h18" /></svg>
      </div>
    </div>
    <div class="tag-content">
      <%# <div class="tag-t">■タスク■</div> %>
      <div class="tag-lists">
        <%= link_to "Task New", new_home_task_path(@home) %>／<%= link_to "Task list", home_tasks_path(@home) %>
          <% @tasks.each do |task| %>
            <li class="post-lists">
              【<%= task.deadline %>】 <%= task.task %> （ <%= task.responsible_person %> ）
            </li>
          <% end %>
      </div>
    </div>
  </div>

  <div class="tag-cell">
    <div class="tag-left">
      <div class="tag-title">LIST</div>
      <div class="tag-addition tag-blue">
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-menu">
        <path d="M3 12h18M3 6h18M3 18h18" /></svg>
      </div>
    </div>
    <div class="tag-content">
      <%# <div class="tag-t">■リスト■</div> %>
      <div class="tag-lists">リスト表示</div>
    </div>
  </div>

  <div class="tag-cell">
    <div class="tag-left">
      <div class="tag-title">MEMO</div>
      <div class="tag-addition tag-pink">
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-menu">
        <path d="M3 12h18M3 6h18M3 18h18" /></svg>
      </div>
    </div>
    <div class="tag-content">
      <%# <div class="tag-t">■メモ■</div> %>
      <div class="tag-lists">メモ表示</div>
    </div>
  </div>

   <div class="tag-cell">
    <div class="tag-title">kari</div>
    <div class="tag-content">
     <div class="tag-t">かり</div>
             <div class="tag-addition tag-yellow">＋</div>
     <div class="tag-lists">リスト表示</div>
    </div>
   </div>

   <div class="tag-cell">
    <div class="tag-title">かり</div>
    <div class="tag-content">
     <div class="tag-t">After You</div>
             <div class="tag-addition tag-purple">＋</div>
     <div class="book-author">by Jojo Moyes</div>
     <div class="rate">
      <fieldset class="rating dark-purp">
       <input type="checkbox" id="star21" name="rating" value="5" />
       <label class="full" for="star21"></label>
       <input type="checkbox" id="star22" name="rating" value="4" />
       <label class="full" for="star22"></label>
       <input type="checkbox" id="star23" name="rating" value="3" />
